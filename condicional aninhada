#include <stdio.h>
int main() {
int num;
printf ("digite um numero: ");
scanf ("%i", &num);
if (num > 21){
printf ("maior que 21");
}
else{
if (num == 21){
printf("sao iguais");
}
else{
printf("diferente");
}
